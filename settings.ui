        <property name="upper">99999</property>
        <property name="step_increment">1</property>
        <property name="page_increment">5</property>
    </object>
    <object class="GtkAdjustment" id="right-offset-adjustment">
        <property name="lower">50</property>
        <property name="upper">99999</property>
        <property name="step_increment">1</property>
        <property name="page_increment">5</property>
